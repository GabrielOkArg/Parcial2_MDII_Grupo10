#funciones/multiplicar.py
def multiplicar(a, b):
 """Devuelve el producto de dos n√∫meros."""
 return a * b
#tests/test_multiplicar.py
from funciones.multiplicar import multiplicar                                                                                                   def test_multiplicar():
 assert multiplicar(3, 4) == 12
 assert multiplicar(-2, 5) == -10
 